<h1>New vbanner</h1>

<% form_for(@vbanner, :html => {:multipart => true}) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :link %><br />
    <%= f.text_field :link %>
  </p>
    <p>
    <%= f.label :vimage %><br />
    <%= f.file_field :vimage %>
  </p>

  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', vbanners_path %>